package edu.learn.recursion;
/*
 * Simple recursive fiboncci calculation
 */
public class Fibonacci_1 {
	static int fib(int n) {
		if(n <= 0 || n == 1 || n == 2) {
			return 1;
		}else {
			return fib(n-1)+fib(n-2);
		}
	}
	public static void main(String[] args) {
		System.out.println("Fib of "+fib(116));
	}

}
